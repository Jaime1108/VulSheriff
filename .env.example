# Copy this file to `.env` and fill in your values

# Required for API calls to OpenRouter
OPENROUTER_API_KEY=sk-or-your-key-here

# Flask session/CSRF secret key (change to a long random string)
FLASK_SECRET_KEY=change-this-secret

# Optional: enable verbose logs
VULNSHERIF_DEBUG=1
VULNSHERIF_LOG_LEVEL=DEBUG

# Optional: change port (default 5000)
# PORT=5000
